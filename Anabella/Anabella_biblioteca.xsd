<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema">
    <element name="biblioteca">
        <complexType>
            <sequence>
                <element name="libros"/>
                    <complexType>
                        <sequence>
                            <element name="libro" minOccurs="1" maxOccurs="unbounded">
                                <complexType>
                                    <sequence>
                                        <element name="titulo" type="string"/>
                                        <element name="sinopsis" type="string"/>
                                        <element name="indice" type="xs:string">
                                            <complexType>
                                                <sequence>
                                                    <elment name="item" minOccurs="1" use="required">
                                                        <complexType>
                                                            <simpleContent>
                                                                <simpleType>
                                                                    <restriction base="integer">
                                                                    <totalDigits value="4"></totalDigits>
                                                                        <atributte name="pagina" type="integer" use="required"></atributte>
                                                                    </restriction>
                                                                </simpleType>   
                                                            </simpleContent>                                                     
                                                        </complexType>
                                                    </elment>
                                                </sequence>
                                            </complexType>
                                        </element>
                                        <element name="capitulo" type="xs:string"/>
                                            <complexType>
                                                <sequence>
                                                    <element name="contenido">
                                                        <complexType>
                                                            <simpleContent>                                                      
                                                                <atributte name="enlace" type="string" use="required"></atributte>
                                                            </simpleContent>
                                                        </complexType>
                                                    </element>
                                                </sequence>
                                                    <atributte name="id_capitulo" use="required"></atributte>
                                                    <atributte name="titulo_cap" use="required"></atributte>
                                                    <atributte name="paginas" use="required"></atributte>                                              
                                            </complexType> 
                                    </sequence>                                   
                                        <atributte name="id" type="string" use="required" default="1"></atributte>
                                        <atributte name="tipo_clase" tipe="string" use="required"></atributte>                                    
                                </complexType>
                            </element> 
                        </sequence>
                    </complexType>                     
                <element name="revistas" type="string"/>
                    <complexType>
                        <element name="revista" minOccurs="1" maxOccurs="unbounded">
                            <complexType>
                                <sequence>
                                    <element name="titulo_rev" type="string"/>
                                    <element name="numero" type="integer"/>
                                    <element name="indice_rev" type="string"/>
                                    <element name="indice_rev" type="string"/>
                                        <complexType>
                                            <element name="item" type="string">
                                                <complexType>
                                                    <simpleContent>
                                                        <simpleType>
                                                            <restriction base="integer">
                                                            <totalDigits value="4"></totalDigits>
                                                                <atributte name="pagina" type="integer" use="required"></atributte>
                                                            </restriction>
                                                        </simpleType>   
                                                    </simpleContent>          
                                                </complexType>
                                            </element>
                                        </complexType>
                                    <element name="secciones" type="string">
                                        <complexType>
                                            <element name="seccion_r" type="string" use="required">
                                                <complexType>
                                                    <sequence>
                                                        <atributte name="titulo" type="string" use="required"></atributte>
                                                        <atributte name="paginas" type="integer" use="required"></atributte>
                                                    </sequence>
                                                </complexType>                                            
                                            <element name="contenido_rev" type="string" use="required"/>
                                                <complexType>
                                                    <simpleContent>
                                                        <atributte name="enlace" type="string" use="required"/>
                                                    </simpleContent>
                                                </complexType>
                                            <element name="paginas" type="integer" use="required"/>
                                            </element>
                                        </complexType>  
                                    </element> 
                                </sequence>
                                <atributte name="id" type="string" use="required"></atributte>  
                                <atributte name="tipo_clase" type="string" use="required"></atributte>                            
                            </complexType>
                        </element>
                    </complexType>
                </element>
                <element name="periodicos" type="string">   
                    <complexType>     
                        <sequence>                         
                            <element name="periodico" minOccurs="1" maxOccurs="unbounded">
                                <complexType>
                                    <sequence>
                                        <element name="secciones" type="string">
                                            <complexType>
                                                <sequence>
                                                    <element name="seccion_per" type="string"> 
                                                        <complexType>
                                                            <simpleContent>
                                                                <atributte name="tipo_seccion" type="string" use="required"/>                                                           
                                                            </simpleContent>
                                                        </complexType>
                                                    </element>                                                                              
                                                    <element name="articulo" type="xs:string">
                                                        <complexType>
                                                            <simpleContent>
                                                                <atributte name="autor" type="string" use="required"></atributte>
                                                            </simpleContent>
                                                        </complexType>
                                                    </element>
                                                    <element name="contenido" type="xs:string"> 
                                                        <complexType>
                                                            <simpleContent>
                                                                <atributte name="enlace" tyep="string" use="required"></atributte>
                                                            </simpleContent>
                                                        </complexType>
                                                    </element>
                                                </sequence>
                                            </complexType> 
                                        </element>                                                                                                                 
                                        <atributte name="id" type="string" use="required"></atributte>
                                        <atributte name="tipo_clase" type="string" use="required"></atributte>
                                        <atributte Name="fecha_publicacion" type="string" use="required"></atributte>
                                </complexType>
                            </element>
                        </sequence>
                    </complexType>
                </element>                
            </sequence>
        </complexType>
    </element>         
</schema>
                                                              
                
               